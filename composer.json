{
    "name": "cakephp-brasil/cart",
    "description": "Cart plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0",
        "webdevbr/cart": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Cart\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cart\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
